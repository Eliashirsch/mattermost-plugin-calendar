{
    "id": "com.dmkir.calendar",
    "name": "Calendar plugin",
    "description": "",
    "homepage_url": "https://github.com/dmitrijkir/mattermost-plugin-calendar",
    "support_url": "https://github.com/dmitrijkir/mattermost-plugin-calendar/issues",
    "release_notes_url": "",
    "icon_path": "assets/starter-template-icon.svg",
    "version": "0.5.0",
    "min_server_version": "7.1.0",
    "server": {
        "executables": {
            "linux-amd64": "server/dist/plugin-linux-amd64",
            "linux-arm64": "server/dist/plugin-linux-arm64",
            "darwin-amd64": "server/dist/plugin-darwin-amd64",
            "darwin-arm64": "server/dist/plugin-darwin-arm64",
            "windows-amd64": "server/dist/plugin-windows-amd64.exe"
        }
    },
    "webapp": {
        "bundle_path": "webapp/dist/main.js"
    },
    "settings_schema": {
        "header": "",
        "footer": "",
        "settings": [
            {
                "key": "BusinessStartTime",
                "display_name": "Business start time by UTC",
                "type": "text",
                "help_text": "Business start time by UTC for exp: 08:00",
                "default": "08:00",
                "placeholder": "08:00"
            },
            {
                "key": "BusinessEndTime",
                "display_name": "Business end time by UTC",
                "type": "text",
                "help_text": "Business end time by UTC for exp: 19:00",
                "default": "19:00",
                "placeholder": "19:00"
            },
            {
                "key": "BusinessDays",
                "display_name": "Business days",
                "type": "text",
                "help_text": "Business days for exp: '1,2,3,4,5' from mon to fri",
                "default": "1,2,3,4,5",
                "placeholder": "1"
            }
        ]
    }
}
